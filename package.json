{
  "name": "lambda-voie",
  "type": "module",
  "version": "0.12.5",
  "packageManager": "pnpm@8.14.0",
  "description": "Routing/middleware engine for AWS Lambda",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/namesmt",
  "homepage": "https://github.com/namesmt/lambda-voie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namesmt/lambda-voie.git"
  },
  "bugs": "https://github.com/namesmt/lambda-voie/issues",
  "keywords": [
    "api",
    "aws",
    "engine",
    "framework",
    "lambda",
    "middleware",
    "plugin",
    "router",
    "routing"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins.d.mts",
      "import": "./dist/plugins.mjs"
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=dev tsx src/index.ts",
    "watch": "NODE_ENV=dev tsx watch src/index.ts",
    "stub": "unbuild --stub",
    "dev": "pnpm run watch",
    "play": "pnpm run stub && pnpm run --filter playground dev",
    "play:useBuild": "pnpm run build && pnpm run --filter playground dev",
    "lint": "eslint .",
    "test": "vitest",
    "test:types": "tsc --noEmit --skipLibCheck",
    "check": "pnpm lint && pnpm test:types && vitest run --coverage",
    "build": "unbuild",
    "release": "pnpm dlx changelogen@latest --release --push --publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@namesmt/utils": "^0.2.0",
    "defu": "^6.1.4",
    "destr": "^2.0.2",
    "filter-obj": "^5.1.0",
    "find-my-way": "^8.1.0",
    "pino": "8.16.2",
    "pino-lambda": "^4.4.0",
    "readable-http-codes": "^1.1.1",
    "std-env": "^3.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@types/node": "^20.10.7",
    "@unocss/eslint-plugin": "^0.58.3",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "pino-pretty": "^10.3.1",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.32",
    "vitest": "^1.1.3"
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@latest"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
