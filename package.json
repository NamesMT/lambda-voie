{
  "name": "lambda-voie",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@8.7.6",
  "description": "Routing/middleware engine for AWS Lambda",
  "author": "NamesMT <dangquoctrung123@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/namesmt",
  "homepage": "https://github.com/namesmt/lambda-voie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namesmt/lambda-voie.git"
  },
  "bugs": "https://github.com/namesmt/lambda-voie/issues",
  "keywords": [
    "aws",
    "lambda",
    "router",
    "routing",
    "middleware",
    "engine"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "pnpm start-watch",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "pnpm dlx changelogen@latest --release --push --publish",
    "start": "cross-env isLocal=true jiti src/index.ts | pino-pretty",
    "start-watch": "cross-env isLocal=true nodemon src/index.ts | pino-pretty",
    "test": "cross-env isLocal=true vitest --test-timeout=0",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "defu": "^6.1.2",
    "filter-obj": "^5.1.0",
    "find-my-way": "^7.6.2",
    "pino": "^8.15.1",
    "pino-lambda": "^4.2.1",
    "readable-http-codes": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@types/node": "^20.7.0",
    "@unocss/eslint-config": "^0.55.7",
    "@vitest/coverage-v8": "^0.34.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.14",
    "jiti": "^1.20.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "pnpm": "^8.7.6",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.20",
    "vitest": "^0.34.5"
  },
  "pnpm": {
    "overrides": {
      "has": "npm:@nolyfill/has@latest"
    }
  }
}
